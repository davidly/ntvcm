CZ	= e:\CROSS
I	= e:\CROSS\I
INCZ	= $(I)\ERRNO.H $(I)\FCNTL.H IO.H
O	= SCANF.O FSCANF.O SSCANF.O SCAN.O PRINTF.O FPRINTF.O SPRINTF.O \
