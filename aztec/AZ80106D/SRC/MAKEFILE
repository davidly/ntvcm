CZ : \CZ.LIB
	@echo The library "CZ" is up to date now.

\CZ.LIB : CZ.LIB
	copy CZ.LIB \*.*
	fdate \CZ.LIB

CZ.LIB : $(O)
	del CZ.LIB
	$(CZ)\libutil -o CZ.LIB $(O)

CLOSE.O : CLOSE.C $(INCZ)
	$(CZ)\ccz -a -i$(I) CLOSE.C
	$(CZ)\as80 -ZAP -o CLOSE.O CLOSE.ASM

